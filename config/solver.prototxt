solver_mode: SIMULATED_ANNEALING
simulation_mode: EXPM

node_penality: 0.0001
edge_penality: 0.0002

model_param {

  min_states: 3
  max_states: 10
  mu: 0
  std: 2

  mutation {
    add_edge: 0.1
    add_node: 0.1
    rm_edge: 0.1
    rm_node: 0.1
    update_std: 0.2
    update_prob: 0.2
  }

}

sa_param {
  k_max: 100000
  n_chains: 10
  step: 1000
  gamma: 0.9
  t0: 0.001
  display: 1000
}
